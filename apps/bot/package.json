{
  "name": "bot",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "check-types": "tsc --noEmit",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/main.ts --outfile bot",
    "dev": "bun run --hot src/main.ts",
    "start": "bun run dist/src/main.js",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --write ."
  },
  "dependencies": {
    "db": "*",
    "dotenv": "^17.2.1",
    "elysia": "^1.3.8",
    "grammy": "^1.37.0",
    "inngest": "^3.40.1",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}